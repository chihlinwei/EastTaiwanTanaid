names(cs)[2] <- "Month"
# Average by site, Month, and microhabitat
cs_avg <- aggregate(cs[,-1:-6], by=list(cs$Site, cs$Month, cs$Microhabitat, cs$Microhabitat2), FUN=mean, na.rm=TRUE)
names(cs_avg)[1:4] <- c("Site", "Month", "Microhabitat", "Microhabitat2")
